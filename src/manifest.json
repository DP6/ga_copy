{
    "name": "GA Copy and Paste",
    "version": "1.1",
    "description": "Copy and Paste Goals and Filters for Google Analytics",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "page_action": {
        "default_title": "GA copy & paste",
        "default_icon": "icon16.png",
        "popup" : "popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "background_page": "background.html",
    "content_scripts":[
        {
        "matches": [
            "http://www.google.com/analytics/web/*",
            "https://www.google.com/analytics/web/*"
        ],
        "js":[
            "js/ga_cp.js",
            "js/ga_paste.js",
            "js/ga_dispatcher.js"
        ],
        "run_at": "document_end"
        }
    ],
    "content_security_policy": "default-src 'self' https://ssl.google-analytics.com https://apis.google.com"
}

